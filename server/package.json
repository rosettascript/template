{
  "name": "template-server",
  "version": "1.0.0",
  "description": "Template server application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "setup": "npm install && npm run build && npm run migrate && npm run seed && npm run optimize",
    "migrate": "ts-node src/scripts/database/migrate.ts",
    "seed": "ts-node src/scripts/database/seed.ts",
    "optimize": "ts-node src/scripts/database/optimize.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["template", "server", "api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^5.8.3",
    "multer": "^2.0.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.0.1",
    "nodemailer": "^7.0.6",
    "winston": "^3.11.0",
    "prom-client": "^15.0.0",
    "express-prometheus-middleware": "^1.2.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.2",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
